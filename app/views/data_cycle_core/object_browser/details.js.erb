$('#object_browser_<%= "#{params[:prefix]}#{sanitize_to_id(params[:key])}" %> .item-info-scrollable .has-tip').each(function(){
  $('#' + $(this).data('toggle')).remove();
});

<% I18n.with_locale(params[:locale] || I18n.locale) do %>
  $('#object_browser_<%= "#{params[:prefix]}#{sanitize_to_id(params[:key])}" %> .item-info-scrollable').html('<%= j render_linked_details(key: params[:key], definition: params[:definition], parameters: { object: @object, options: params[:options] }) %>').removeClass('dc-fd-initialized');
<% end %>

$('#object_browser_<%= "#{params[:prefix]}#{sanitize_to_id(params[:key])}" %> .item-info-scrollable');
