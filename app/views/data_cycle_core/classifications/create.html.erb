<% if @object.is_a?(DataCycleCore::ClassificationTreeLabel) %>
  <%= render 'data_cycle_core/classifications/classification_tree_label', tree_label: @object %>

  <%= render 'data_cycle_core/classifications/new_classification_tree_label' %>
<% elsif @object.is_a?(DataCycleCore::ClassificationTree) %>
  <%= render 'data_cycle_core/classifications/classification_tree', classification_tree_label: @classification_tree_label, classification_tree: @object, ancestors: @object.present? ? @object.ancestors.pluck(:id).reverse : [], type: 'direct' %>

  <%= render('data_cycle_core/classifications/new_classification_alias', classification_tree_label: @classification_tree_label, classification_tree: @classification_tree) %>
<% end %>
