<% if @object.is_a? DataCycleCore::ClassificationTreeLabel %>
  <%= render 'classification_tree_label', tree_label: @object %>
<% else %>
  <%= render 'classification_tree', classification_tree_label: @object.classification_tree.classification_tree_label, classification_tree: @object.classification_tree, ancestors: @object.classification_tree.present? ? @object.classification_tree.ancestors.pluck(:id).reverse : [], type: 'direct' %>
<% end %>
